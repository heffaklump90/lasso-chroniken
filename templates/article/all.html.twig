{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-md-12">
            <h1>Articles</h1>

            {% for article in articles %}
            <h2>{{ article.title }}</h2>
            <div class="article-content">
                {{ article.content | raw }}
            </div>
            {% endfor %}
        </div>
    </div>
{% endblock %}
